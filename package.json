{
  "name": "npm-lib-rollup-template",
  "version": "0.0.0",
  "description": "npm-lib-rollup-template",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "rollup -c",
    "build": "rollup",
    "build:tsc": "cross-env NODE_ENV=development tsc --build tsconfig.json",
    "dts": "tsc --emitDeclarationOnly -p tsconfig.dts.json",
    "test:dev": "vitest",
    "test": "vitest run",
    "init:rename": "node scripts/init/rename.js",
    "init:bin": "node scripts/init/bin.js",
    "clean": "node scripts/clean.js",
    "format": "pnpm prettier --check \"src/**/*.{ts,js}\" -w",
    "ls:pack": "npm pack --dry-run",
    "prepare": "ts-patch install -s",
    "semantic-release": "semantic-release"
  },
  "packageManager": "pnpm@9.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonofmagic/npm-lib-rollup-template.git"
  },
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [],
  "author": "SonOfMagic <qq1324318532@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sonofmagic/npm-lib-rollup-template/issues"
  },
  "homepage": "https://github.com/sonofmagic/npm-lib-rollup-template#readme",
  "devDependencies": {
    "@icebreakers/eslint-config": "^0.3.17",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/recommended": "^1.0.7",
    "@types/klaw": "^3.0.6",
    "@types/lodash": "^4.17.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.0.0",
    "@types/semantic-release": "^20.0.6",
    "@vitest/coverage-v8": "^2.0.5",
    "cross-env": "^7.0.3",
    "defu": "^6.1.4",
    "del": "^7.1.0",
    "eslint": "9.8.0",
    "klaw": "^4.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "only-allow": "^1.2.1",
    "rollup": "^4.19.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-transform-paths": "^3.4.7",
    "vitest": "^2.0.5"
  }
}